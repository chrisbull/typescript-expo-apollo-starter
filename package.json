{
  "name": "typescript-expo-apollo-starter",
  "main": "node_modules/expo/AppEntry.js",
  "version": "0.1.0",
  "author": "SiteAppy - Chris Bull",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "tslint src/**/*.ts",
    "tsc": "tsc",
    "clean": "rimraf build",
    "build": "yarn run clean && yarn run tsc",
    "watch": "yarn run build -w",
    "watchAndRunAndroid": "concurrently \"yarn run watch\" \"yarn run android\"",
    "buildRunAndroid": "yarn run build && yarn run watchAndRunAndroid ",
    "watchAndRunIOS": "concurrently \"yarn run watch\" \"yarn run ios\"",
    "buildRunIOS": "yarn run build && yarn run watchAndRunIOS ",
    "watchAndStart": "concurrently \"yarn run watch\" \"yarn run start\"",
    "buildAndStart": "yarn run build && yarn run watchAndStart",
    "gen-app-config": "ts-node generate-app-config.ts"
  },
  "dependencies": {
    "@expo/samples": "2.1.1",
    "@expo/vector-icons": "8.0.0",
    "apollo-cache-inmemory": "1.3.10",
    "apollo-client": "2.4.6",
    "apollo-link-schema": "1.1.1",
    "expo": "31.0.5",
    "graphql": "14.0.2",
    "graphql-tag": "2.10.0",
    "graphql-tools": "4.0.3",
    "moment": "2.22.2",
    "react": "16.5.0",
    "react-apollo": "2.3.1",
    "react-dom": "^16.6.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.1.tar.gz",
    "react-native-dotenv": "^0.2.0",
    "react-native-screens": "^1.0.0-alpha.16",
    "react-navigation": "3.0.0",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-typescript": "^7.1.0",
    "@divyenduz/ts-graphql-plugin": "0.1.0",
    "@types/expo": "30.0.1",
    "@types/expo__vector-icons": "6.2.3",
    "@types/fbemitter": "2.0.32",
    "@types/graphql": "14.0.3",
    "@types/jest": "23.3.9",
    "@types/moment": "2.13.0",
    "@types/react": "16.7.6",
    "@types/react-native": "0.57.10",
    "@types/react-navigation": "2.13.2",
    "@types/react-test-renderer": "16.0.3",
    "@types/redux": "3.6.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-expo": "5.0.0",
    "concurrently": "^4.0.1",
    "dotenv-cli": "^1.4.0",
    "jest": "23.6.0",
    "jest-expo": "31.0.0",
    "metro-react-native-babel-preset": "0.49.2",
    "npm-run-all": "^4.1.3",
    "prettier": "1.15.2",
    "prettier-tslint": "0.4.0",
    "react-native-scripts-ts": "1.15.0",
    "react-native-typescript-transformer": "1.2.10",
    "react-test-renderer": "16.6.3",
    "ts-jest": "23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.16.0",
    "tslint-config-standard": "8.0.1",
    "tslint-eslint-rules": "5.4.0",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "3.6.0",
    "typescript": "3.1.6"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": false
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
